{
  "name": "@astrojs/fast",
  "version": "2.0.2",
  "description": "Use Fast components within Astro",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/astro.git",
    "directory": "packages/integrations/fast"
  },
  "keywords": [
    "astro-integration",
    "astro-component",
    "renderer",
    "fast"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://docs.astro.build/en/guides/integrations-guide/lit/",
